<h1><%= titleForm %></h1>
<form action="/<%= action %>" method="post">
    <label for="task">
        <input type="text" name="task" id="task" placeholder="Insira uma tarefa" value="<%= value %>">
        <button id="<%= idBtn %>" type="submit">
            <%= nameBtn %>
        </button>
    </label>
</form>